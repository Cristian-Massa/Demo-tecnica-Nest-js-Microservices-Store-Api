{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/store-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/store-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/store-gateway",
  "projects": {
    "contracts": {
      "type": "library",
      "root": "libs/contracts",
      "entryFile": "index",
      "sourceRoot": "libs/contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contracts/tsconfig.lib.json"
      }
    },
    "products": {
      "type": "application",
      "root": "apps/products",
      "entryFile": "main",
      "sourceRoot": "apps/products/src",
      "compilerOptions": {
        "tsConfigPath": "apps/products/tsconfig.app.json"
      }
    },
    "store-gateway": {
      "type": "application",
      "root": "apps/store-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/store-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/store-gateway/tsconfig.app.json"
      }
    }
  }
}
